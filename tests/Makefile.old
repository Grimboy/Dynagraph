.SUFFIXES  : .incr .incrout

DYNAGRAPH=../dynagraph
SLOW=cat
INCRS=*.incr

run: *.incr

*.incr: always
	$(SLOW) $@ | $(DYNAGRAPH) > $*.incrout
	$(SLOW) $@ | $(DYNAGRAPH) -a superengines=shapegen > $*.super.incrout
	$(SLOW) $@ | $(DYNAGRAPH) -a intermediate=true > $*.imed.incrout
	$(SLOW) $@ | $(DYNAGRAPH) -a superengines=shapegen -a intermediate=true > $*.super.imed.incrout

always : 

clean:
	rm -f *.incrout
